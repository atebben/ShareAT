function consentCookies(n){setCookie("CookieConsent","1",365,"/",n)}function setCookie(n,t,i,r,u){var e=new Date,o,f;e.setTime(e.getTime()+i*864e5);o="expires="+e.toGMTString();f=n+"="+t+"; "+o+";";r&&(f+="path="+r+";");u&&(f+="domain="+u+";");document.cookie=f}$(document).ready(function(){$(".cookie-notification button").click(function(){consentCookies($(this).data("domain"))});$("body").has(".cookie-notification").addClass("has-cookieconsent")})